    .page-container

      //
           BEGIN SIDEBAR 
      .page-sidebar-wrapper
            //
               DOC: Set data-auto-scroll="false" to disable the sidebar from auto scrolling/focusing 
            //
               DOC: Change data-auto-speed="200" to adjust the sub menu slide up/down speed 
            .page-sidebar.navbar-collapse.collapse
              //
                 BEGIN SIDEBAR MENU 
              ul.page-sidebar-menu(data-auto-scroll='true', data-slide-speed='200')
                //
                   DOC: To remove the sidebar toggler from the sidebar you just need to completely remove the below "sidebar-toggler-wrapper" LI element 
                li.sidebar-toggler-wrapper(style="margin-bottom: 15px;")
                  //
                     BEGIN SIDEBAR TOGGLER BUTTON 
                  .sidebar-toggler
                  //
                     END SIDEBAR TOGGLER BUTTON 

                //
                   END FRONTEND THEME LINKS 
                li.start(class=req_path.indexOf("/club") != -1 ? "active":"")
                  a(href='/venue/clubs')
                    i.icon-home
                    span.title My Clubs
                    span.selected
                li(class=req_path.indexOf("/festival") != -1 ? "active":"")
                  a(href='/venue/festivals')
                    i.icon-star
                    span.title Festivals
                    span.selected
                li(class=req_path.indexOf("/dj") != -1 ? "active":"")
                  a(href='/venue/djs')
                    i.icon-star
                    span.title DJ's
                    span.selected
                if current_user.is_admin
                  li(class=req_path.indexOf("/users") != -1 ? "active":"")
                    a(href='/venue/users')
                      i.icon-users
                      span.title Users
                      span.selected
                li
                  a(href='#')
                    i.icon-bar-chart
                    span.title Statistic
                    span.selected
 
                li.last
                  a(href='#')
                    i.icon-settings
                    span.title Settings
              //
                 END SIDEBAR MENU 
